public class abc {
	public static void main(String[] args) {
      int i,j;
      for(i=0;i<4;i++)
      {
          for (j = 0; j < 4; j++) {
        	  System.out.print("*");		
		}
          System.out.println();
      }
      for(i=0;i<4;i++)
      {
    	  for(j=0;j<=i;j++)
    	  {
    		  System.out.print("*");
    	  }
    	  System.out.println();
      }
      int n=1;
      while(n<10)
      {
    	 int m=1;
    	  while(m<=n)
    	  {
    		  System.out.print(m+"*"+n+"="+(n*m)+"\t");
    		  m++;
    	  }
    	  System.out.println();
    	  n++;
      }
      int day, num=1;
      for(day=1;day<10;day++)
      {
    	  num=(num+1)*2;
      }
      System.out.println("猴子一共摘了"+num+"个桃");
	}

}
